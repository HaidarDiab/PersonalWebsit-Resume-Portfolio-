@model MyWebsite.ViewModels.ProfileFormViewModel
@{
    ViewBag.Title = "EditDeveloperskill";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div style="margin-left: 20px">

<h2>EditDeveloperskill</h2>

@using (Html.BeginForm("EditDeveloperskill", "Resumes"))
{

    <div class="form-group">
        @Html.LabelFor(m => m.DeveloperSkill.Description)
        @Html.TextAreaFor(m => m.DeveloperSkill.Description, new { @class = "form-control" })
    </div>


    @Html.HiddenFor(m => m.DeveloperSkill.Id)
    @Html.HiddenFor(m => m.ApplicationUserId)
    @Html.AntiForgeryToken()

    <button type="submit" class="btn btn-danger" style="margin: 10px">Save</button>

}

</div>
@model MyWebsite.ViewModels.ProfileFormViewModel
@{
    ViewBag.Title = "IndexDashboardHai";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    


<div class="container row profile-form-dev" style="text-decoration: none;"> 

    @using (Html.BeginForm())
    {
    <h3 class="card card-header">Dashboard</h3>
        <div class="row text-form" style="column-count: 2;">

            <div class="row col-lg-4 indexdashboard-leftside">
               
                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("My Profile", "Index", "Profiles")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Create Profile", "Create", "Profiles")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Summary", "SummaryInsert", "Resumes")
                </div>


                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Education Info", "EducationInsert", "Resumes")
                </div>


                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Project", "Add", "Projects")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Work Experience", "WorkExperienceInsert", "Resumes")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Work Specialist", "WorkSpecialistInsert", "Resumes")
                </div>

            </div>



            <div class="row col-lg-4 indexdashboard-rightside">
                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Edit Work Specialist", "IndexWorkSpecialist", "Resumes")
                </div>


                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Personal Skills", "PersonalSkillsInsert", "Resumes")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Software Skills", "SoftwareSkillsInsert", "Resumes")
                </div>
                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Developers Skills", "DevelopersSkillsInsert", "Resumes")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Languges", "LanguageSkillsInsert", "Resumes")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Add Social Links", "SocialLinkInsert", "Resumes")
                </div>

                <div class="form-group card card-body shadow shadow-lg">
                    @Html.ActionLink("Edit Social Links", "SocialLinkIndex", "Resumes")
                </div>
            </div>

            @Html.HiddenFor(m => m.ApplicationUserId)

        </div>
    }

</div>